
<tr class="row" key="{{key}}">
	<td>
		<h3><span onclick="uinv.FCM.configMgr.model.viewpoint.objectRename(this,'{{key}}');" class="name">{{name}}</span></h3>
	</td>
	
	{{#each data}}
		<td>
			<input class="w50" type="text" key="{{@key}}" name="{{../key}}" cate="viewpoint" path="viewpoint" />
		</td>
	{{/each}}
	
	<td>
		<a onclick="uinv.FCM.configMgr.model.viewpoint.objectDelete(this, '{{key}}');" href="javascript:void(0);"><s>删除</s></a>
	</td>
</tr>